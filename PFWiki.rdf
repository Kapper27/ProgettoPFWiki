        <rdfs:comment xml:lang="it">Una temibile corona di corna circonda la testa di questo possente drago e spesse scaglie rosso fuoco ne ricoprono il lungo corpo.</rdfs:comment>
        <rdfs:label xml:lang="it">Drago rosso giovane</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#DragonFight_Drago -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#DragonFight_Drago">
        <rdf:type rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#ParticipantRole"/>
        <objectParticipating rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Drago"/>
        <participatingInEvent rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Incontro_2"/>
        <roleOfParticipant rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Nemico"/>
        <rdfs:comment xml:lang="it">Drago rosso giovane presente nella &quot;Battaglia col drago&quot;</rdfs:comment>
        <rdfs:label xml:lang="it">Drago rosso giovane (Battaglia col drago)</rdfs:label>
    </owl:NamedIndividual>
    


