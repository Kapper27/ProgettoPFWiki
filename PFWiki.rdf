    <!-- http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Equipaggiamento_magico -->

    <owl:Class rdf:about="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Equipaggiamento_magico">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Equipaggiamento"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#èMagico"/>
                        <owl:hasValue rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</owl:hasValue>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Equipaggiamento"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Oggetto_magico"/>
