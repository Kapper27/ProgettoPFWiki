@prefix : <http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki> .

<http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki> rdf:type owl:Ontology ;
                                                             <http://purl.org/dc/elements/1.1/creator> "Andrea Pandolfi" ;
                                                             <http://purl.org/dc/elements/1.1/date> "2025-01-26" ;
                                                             <http://purl.org/dc/elements/1.1/description> "Un'ontologia per rappresentare il gioco di ruolo Pathfinder, le campagne basate su questo set di regole e gli eventi di quest'ultime."@it ;
                                                             <http://purl.org/dc/elements/1.1/title> "Pathfinder Wiki Ontology"@en ;
                                                             <http://purl.org/vocab/vann/preferredNamespacePrefix> "http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#" ,
                                                                                                                   "pfwiki" ;
                                                             rdfs:label "PFWiki"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2003/06/sw-vocab-status/ns#term_status
<http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#appartieneCampagna
:appartieneCampagna rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :Evento ;
                    rdfs:range :Campagna ;
                    rdfs:comment "Rappresenta l'appartenenza di un evento ad una determinata campagna"@it ;
                    rdfs:label "Appartiene alla campagna"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#concedeAbilità
:concedeAbilità rdf:type owl:ObjectProperty ,
                         owl:TransitiveProperty ;
                rdfs:domain :Classe ;
                rdfs:range :Abilità ;
                rdfs:comment "Rappresenta il fatto che una determinata abilità viene concessa da una determinata classe"@it ;
                rdfs:label "Concede abilità"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#conosceIncantesimo
:conosceIncantesimo rdf:type owl:ObjectProperty ;
                    rdfs:domain :Personaggio ;
                    rdfs:range :Incantesimo ;
                    rdfs:comment "Rappresenta la capacità di un personaggio di usare un determinato incantesimo"@it ;
                    rdfs:label "Conosce incantesimo"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#haAbilità
:haAbilità rdf:type owl:ObjectProperty ;
           rdfs:domain :Personaggio ;
           rdfs:range :Abilità ;
           rdfs:comment "Rappresenta la capacità di un determinato personaggio di usare una determinata abilità"@it ;
           rdfs:label "Ha abilità"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#haClasse
:haClasse rdf:type owl:ObjectProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Personaggio ;
          rdfs:range :Classe ;
          rdfs:comment "Rappresenta la classe di un determinato personaggio"@it ;
          rdfs:label "Ha classe"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#haGiocatore
:haGiocatore rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Personaggio ;
             rdfs:range :Giocatore ;
             rdfs:comment "Rappresenta l'appartenenza di un personaggio ad un deteminato giocatore"@it ;
             rdfs:label "Ha giocatore"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#haRazza
:haRazza rdf:type owl:ObjectProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Personaggio ;
         rdfs:range :Razza ;
         rdfs:comment "Rappresenta la razza di un personaggio"@it ;
         rdfs:label "Ha razza"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#hasFirstEvent
:hasFirstEvent rdf:type owl:ObjectProperty ,
                        owl:FunctionalProperty ;
               rdfs:domain :Campagna ;
               rdfs:range :Evento ;
               rdfs:comment "Rappresenta il collegamento con il primo evento di una campagna"@it ;
               rdfs:label "Primo evento"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#hasLastEvent
:hasLastEvent rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Campagna ;
              rdfs:range :Evento ;
              rdfs:comment "Rappresenta il collegamento con l'ultimo evento di una campagna"@it ;
              rdfs:label "Ultimo evento"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#hasNextEvent
:hasNextEvent rdf:type owl:ObjectProperty ;
              owl:inverseOf :hasPreviousEvent ;
              rdf:type owl:FunctionalProperty ,
                       owl:AsymmetricProperty ,
                       owl:IrreflexiveProperty ;
              rdfs:domain :Evento ;
              rdfs:range :Evento ;
              rdfs:comment "Rappresenta il collegamento tra un evento e il suo successore nella lista di eventi"@it ;
              rdfs:label "Evento successivo"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#hasPreviousEvent
:hasPreviousEvent rdf:type owl:ObjectProperty ;
                  rdfs:comment "Rappresenta il collegamento tra un evento e il suo predecessore nella lista di eventi"@it ;
                  rdfs:label "Evento precedente"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#objectParticipating
:objectParticipating rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain :ParticipantRole ;
                     rdfs:range :Creatura ;
                     rdfs:comment "Rappresenta il collegamento tra una creatura ed una sua \"istanza\" in un evento"@it ;
                     rdfs:label "Creatura partecipante"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#participatingInEvent
:participatingInEvent rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain :ParticipantRole ;
                      rdfs:range :Incontro ;
                      rdfs:comment "Collegamento tra l'istanza di una creatura e uno scontro"@it ;
                      rdfs:label "Partecipa allo scontro"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#possiede
:possiede rdf:type owl:ObjectProperty ;
          rdfs:domain :Creatura ;
          rdfs:range :Oggetto ;
          rdfs:comment "Rappresenta il fatto che una determinata creatura possiede un determinato oggetto"@it ;
          rdfs:label "Possiede oggetto"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#roleOfParticipant
:roleOfParticipant rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :ParticipantRole ;
                   rdfs:range :Role ;
                   rdfs:comment "Rappresenta il ruolo di un partecipante rispetto ad uno scontro"@it ;
                   rdfs:label "Ruolo del partecipante"@it .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#concedeIncantesimiArcani
:concedeIncantesimiArcani rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Classe ;
                          rdfs:range xsd:boolean ;
                          rdfs:comment "Stabilisce se la classe concede o meno incantesimi arcani"@it ;
                          rdfs:label "Concede incantesimi arcani"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#concedeIncantesimiDivini
:concedeIncantesimiDivini rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Classe ;
                          rdfs:range xsd:boolean ;
                          rdfs:comment "Stabilisce se la classe concede o meno incantesimi divini"@it ;
                          rdfs:label "Concede incantesimi divini"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#dataEvento
:dataEvento rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Evento ;
            rdfs:range xsd:dateTime ;
            rdfs:comment "Rappresenta la data di un evento"@it ;
            rdfs:label "Data evento"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#livelloRichiestoAbilità
:livelloRichiestoAbilità rdf:type owl:DatatypeProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain :Abilità ;
                         rdfs:range xsd:integer ;
                         rdfs:comment "Stabilisce il livello richiesto per ottenere una determinata abilità"@it ;
                         rdfs:label "Livello richiesto abilità"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#valoreLivelloIncantesimo
:valoreLivelloIncantesimo rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Incantesimo ;
                          rdfs:range xsd:integer ;
                          rdfs:comment "Stabilisce il livello richiesto per ottenere l'incantesimo"@it ;
                          rdfs:label "Livello richiesto incantesimo"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#valoreLivelloPersonaggio
:valoreLivelloPersonaggio rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain :Personaggio ;
                          rdfs:range xsd:integer ;
                          rdfs:comment "Stabilisce il livello del personaggio"@it ;
                          rdfs:label "Livello personaggio"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#èMagico
:èMagico rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :Oggetto ;
         rdfs:range xsd:boolean ;
         rdfs:comment "Stabilisce se l'oggetto ha proprietà magiche"@it ;
         rdfs:label "È magico"@it .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Abilità
:Abilità rdf:type owl:Class ;
         rdfs:comment "Una capacità straordinaria concessa da una classe"@it ;
         rdfs:label "Abilità"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Campagna
:Campagna rdf:type owl:Class ;
          rdfs:comment "Una serie di sessioni di gioco"@it ;
          rdfs:label "Campagna"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Classe
:Classe rdf:type owl:Class ;
        rdfs:comment "La professione di un personaggio"@it ;
        rdfs:label "Classe"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Creatura
:Creatura rdf:type owl:Class ;
          rdfs:comment "Entità fisica"@it ;
          rdfs:label "Creatura"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Equipaggiamento
:Equipaggiamento rdf:type owl:Class ;
                 rdfs:subClassOf :Oggetto ;
                 rdfs:comment "Un oggetto equipaggiabile/indossabile"@it ;
                 rdfs:label "Equipaggiamento"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Equipaggiamento_magico
:Equipaggiamento_magico rdf:type owl:Class ;
                        owl:equivalentClass [ owl:intersectionOf ( :Equipaggiamento
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty :èMagico ;
                                                                     owl:hasValue "true"^^xsd:boolean
                                                                   ]
                                                                 ) ;
                                              rdf:type owl:Class
                                            ] ;
                        rdfs:subClassOf :Equipaggiamento ,
                                        :Oggetto_magico ;
                        rdfs:comment "Un oggetto equipaggiabile/indossabile con proprietà magiche"@it ;
                        rdfs:label "Equipaggiamento magico"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Evento
:Evento rdf:type owl:Class ;
        owl:equivalentClass <https://schema.org/Event> ;
        rdfs:subClassOf <https://schema.org/Thing> ;
        rdfs:comment "Un avvenimento rilevante di una campagna"@it ;
        rdfs:label "Evento"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Giocatore
:Giocatore rdf:type owl:Class ;
           owl:equivalentClass <http://xmlns.com/foaf/0.1/Person> ;
           rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                           <http://xmlns.com/foaf/0.1/Agent> ;
           rdfs:comment "Una persona che gioca ad un gioco di ruolo"@it ;
           rdfs:label "Giocatore"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Incantatore
:Incantatore rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:unionOf ( [ owl:intersectionOf ( :Classe
                                                                        [ rdf:type owl:Restriction ;
                                                                          owl:onProperty :concedeIncantesimiArcani ;
                                                                          owl:hasValue "true"^^xsd:boolean
                                                                        ]
                                                                      ) ;
                                                   rdf:type owl:Class
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :concedeIncantesimiDivini ;
                                                   owl:hasValue "true"^^xsd:boolean
                                                 ]
                                               )
                                 ] ;
             rdfs:subClassOf :Classe ;
             owl:disjointWith :Marziale ;
             rdfs:comment "Una classe capace di utilizzare incantesimi"@it ;
             rdfs:label "Incantatore"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#IncantatoreArcano
:IncantatoreArcano rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Incantatore
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :concedeIncantesimiArcani ;
                                                                owl:hasValue "true"^^xsd:boolean
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Incantatore ;
                   owl:disjointWith :Marziale ;
                   rdfs:comment "Una classe capace di utilizzare incantesimi arcani"@it ;
                   rdfs:label "Incantatore arcano"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#IncantatoreDivino
:IncantatoreDivino rdf:type owl:Class ;
                   owl:equivalentClass [ owl:intersectionOf ( :Incantatore
                                                              [ rdf:type owl:Restriction ;
                                                                owl:onProperty :concedeIncantesimiDivini ;
                                                                owl:hasValue "true"^^xsd:boolean
                                                              ]
                                                            ) ;
                                         rdf:type owl:Class
                                       ] ;
                   rdfs:subClassOf :Incantatore ;
                   owl:disjointWith :Marziale ;
                   rdfs:comment "Una classe capace di utilizzare incantesimi concessi da una divinità"@it ;
                   rdfs:label "Incantatore divino"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Incantesimo
:Incantesimo rdf:type owl:Class ;
             rdfs:comment "Capacità magica"@it ;
             rdfs:label "Incantesimo"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Incantesimo_arcano
:Incantesimo_arcano rdf:type owl:Class ;
                    rdfs:subClassOf :Incantesimo ;
                    owl:disjointWith :Incantesimo_divino ;
                    rdfs:comment "Capacità magica di natura arcana"@it ;
                    rdfs:label "Incantesimo arcano"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Incantesimo_divino
:Incantesimo_divino rdf:type owl:Class ;
                    rdfs:subClassOf :Incantesimo ;
                    rdfs:comment "Incantesimo di natura divina"@it ;
                    rdfs:label "Incantesimo divino"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Incontro
:Incontro rdf:type owl:Class ;
          rdfs:subClassOf :Evento ,
                          <https://schema.org/Event> ;
          rdfs:comment "Un evento nel quale è avvenuto un combattimento"@it ;
          rdfs:label "Scontro"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Marziale
:Marziale rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :Classe
                                                     [ rdf:type owl:Class ;
                                                       owl:complementOf :Incantatore
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          rdfs:subClassOf :Classe ;
          rdfs:comment "Una classe senza capacità magiche, che si affida completamente alle capacità fisiche"@it ;
          rdfs:label "Marziale"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Mostro
:Mostro rdf:type owl:Class ;
        rdfs:subClassOf :Creatura ;
        rdfs:comment "Una creatura non giocabile da un giocatore"@it ;
        rdfs:label "Mostro"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Oggetto
:Oggetto rdf:type owl:Class ;
         rdfs:comment "un oggetto"@it ;
         rdfs:label "Oggetto"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Oggetto_magico
:Oggetto_magico rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( :Oggetto
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty :èMagico ;
                                                             owl:hasValue "true"^^xsd:boolean
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:subClassOf :Oggetto ;
                rdfs:label "Oggetto magico"@it ,
                           "Un oggetto con proprietà magiche"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#PG
:PG rdf:type owl:Class ;
    owl:equivalentClass [ owl:intersectionOf ( :Personaggio
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :haClasse ;
                                                 owl:someValuesFrom :Classe
                                               ]
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty :haGiocatore ;
                                                 owl:someValuesFrom :Giocatore
                                               ]
                                             ) ;
                          rdf:type owl:Class
                        ] ;
    rdfs:subClassOf :Personaggio ;
    rdfs:comment "Un personaggio associato ad un giocatore"@it ;
    rdfs:label "Personaggio giocante"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#ParticipantRole
:ParticipantRole rdf:type owl:Class ;
                 rdfs:comment "Una \"istanza\" di una creatura relativa ad un evento"@it ;
                 rdfs:label "Partecipante"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Personaggio
:Personaggio rdf:type owl:Class ;
             rdfs:subClassOf :Creatura ;
             rdfs:comment "Una creatura possibilmente giocabile da un giocatore"@it ;
             rdfs:label "Personaggio"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Razza
:Razza rdf:type owl:Class ;
       rdfs:comment "Razza di una creatura"@it ;
       rdfs:label "Razza"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Role
:Role rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Class ;
                            owl:oneOf ( :Alleato
                                        :Nemico
                                      )
                          ] ;
      rdfs:comment "Ruolo di una creatura rispetto ad un evento"@it ;
      rdfs:label "Ruolo"@it .


###  http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing
<http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Agent
<http://xmlns.com/foaf/0.1/Agent> rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
<http://xmlns.com/foaf/0.1/Person> rdf:type owl:Class ;
                                   rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                                                   <http://xmlns.com/foaf/0.1/Agent> ;
                                   rdfs:comment "A person." ;
                                   rdfs:isDefinedBy <http://xmlns.com/foaf/0.1/> ;
                                   rdfs:label "Person" ;
                                   <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status> "stable" .


###  https://schema.org/Event
<https://schema.org/Event> rdf:type owl:Class ;
                           rdfs:subClassOf <https://schema.org/Thing> ;
                           rdfs:comment "An event happening at a certain time and location, such as a concert, lecture, or festival. Ticketing information may be added via the <a class=\"localLink\" href=\"/offers\">offers</a> property. Repeated events may be structured as separate Event objects."@en ;
                           rdfs:isDefinedBy <https://schema.org/Event> ;
                           rdfs:label "Event"@en .


###  https://schema.org/Thing
<https://schema.org/Thing> rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Alleato
:Alleato rdf:type owl:NamedIndividual ,
                  :Role ;
         rdfs:comment "Entità amichevole rispetto i giocatori"@it ;
         rdfs:label "Alleato"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Andrea
:Andrea rdf:type owl:NamedIndividual ,
                 :Giocatore ,
                 <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ,
                 <http://xmlns.com/foaf/0.1/Agent> ,
                 <http://xmlns.com/foaf/0.1/Person> ;
        rdfs:comment "Giocatore di nome Andrea"@it ;
        rdfs:label "Andrea"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Anello_di_protezione_1
:Anello_di_protezione_1 rdf:type owl:NamedIndividual ,
                                 :Equipaggiamento ,
                                 :Equipaggiamento_magico ,
                                 :Oggetto ,
                                 :Oggetto_magico ;
                        :èMagico "true"^^xsd:boolean ;
                        rdfs:comment "Anello magico, aumenta l'armatura di 1"@it ;
                        rdfs:label "Anello di protezione 1"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Campagna_Esempio_1
:Campagna_Esempio_1 rdf:type owl:NamedIndividual ,
                             :Campagna ;
                    :hasFirstEvent :Incontro_1 ;
                    :hasLastEvent :Incontro_3 ;
                    rdfs:comment "Campagna di pathfinder iniziata il 10/01/2025"@it ;
                    rdfs:label "Destiny calls"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Chierico
:Chierico rdf:type owl:NamedIndividual ,
                   :Classe ,
                   :Incantatore ,
                   :IncantatoreDivino ;
          :concedeIncantesimiDivini "true"^^xsd:boolean ;
          rdfs:comment "Nella fede e nei miracoli del divino, molti trovano un maggior scopo. Chiamati per servire i poteri oltre la comprensione dei mortali, tutti i sacerdoti predicano le meraviglie del creato e provvedono ai bisogni spirituali dei loro fedeli. I chierici sono più che semplici sacerdoti, comunque; questi emissari del divino esaudiscono la volontà delle loro Divinità con la Forza delle Armi o con i poteri Magici loro concessi. Devoti ai principi delle religioni e delle filosofie che li ispirano, questi ecclesiastici cercano di spargere la conoscenza e l'influenza della loro fede. Tuttavia anche se potrebbe sembrare che condividano capacità simili, i chierici dimostrano di essere diversi l'uno dall'altro come gli Dei che servono: alcuni offrono redenzione e guarigione, altri portano legge e giustizia, e altri ancora seminano conflitto e corruzione. I modi dei chierici sono svariati, ma tutti coloro che seguono questa carriera lo fanno con gli alleati più forti ed imbracciano le Armi delle Divinità."@it ;
          rdfs:label "Chierico"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Cura_ferite
:Cura_ferite rdf:type owl:NamedIndividual ,
                      :Incantesimo ,
                      :Incantesimo_divino ;
             :valoreLivelloIncantesimo 1 ,
                                       1 ;
             rdfs:comment "Appoggiando la mano su una creatura vivente, si infonde energia positiva che cura 1d8 danni +1 danno per livello dell'incantatore (massimo +5). Dal momento che i Non Morti sono animati da energia negativa, questo incantesimo infligge loro danni piuttosto che curare le loro ferite. Una creatura Non Morta può applicare la sua Resistenza agli Incantesimi, e tentare un Tiro Salvezza su Volontà per dimezzare i danni subiti."@it ;
             rdfs:label "Cura ferite leggere"@it .


###  http://www.semanticweb.org/andre/ontologies/2025/0/PFWiki#Drago
:Drago rdf:type owl:NamedIndividual ,
                :Creatura ,
                :Mostro ;
       rdfs:comment "Una temibile corona di corna circonda la testa di questo possente drago e spesse scaglie rosso fuoco ne ricoprono il lungo corpo."@it ;
       rdfs:label "Drago rosso giovane"@it .


